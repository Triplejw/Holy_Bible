{
  "expo": {
    "name": "Holy Bible",
    "slug": "holy-bible",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "scheme": "holy-bible",
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
}
